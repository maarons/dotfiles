{
  "mkdir": [
    "~/.config/gtk-3.0",
    "~/.config/tint2"
  ],
  "chmod": {
    "emacs/tmp": "0700",
    "vim/tmp": "0700"
  },
  "symlink": {
    "alsa/asoundrc": "~/.asoundrc",
    "mplayer/" : "~/.mplayer",
    "screen/screenrc": "~/.screenrc",
    "gtk/gtkrc-2.0": "~/.gtkrc-2.0",
    "gtk/settings.ini": "~/.config/gtk-3.0/settings.ini",
    "guile/guile": "~/.guile",
    "mozplugger/": "~/.mozplugger",
    "ruby/gemrc": "~/.gemrc",
    "ruby/irbrc": "~/.irbrc",
    "x/Xdefaults": "~/.Xdefaults",
    "x/xscreensaver": "~/.xscreensaver",
    "x/XCompose": "~/.XCompose",
    "zsh/zshrc": [
      "~/.zshrc",
      "~/.zprofile"
    ],
    "emacs/": "~/.emacs_conf",
    "emacs/tmp/": "~/.emacs_tmp",
    "emacs/emacs": "~/.emacs",
    "vim/": "~/.vim",
    "vim/vimrc": "~/.vimrc",
    "vim/gvimrc": "~/.gvimrc",
    "x/xmonad/": "~/.xmonad",
    "x/xmobarrc": "~/.xmobarrc",
    "x/tint2rc": "~/.config/tint2/tint2rc"
  }
}
