include "/usr/share/X11/locale/en_US.UTF-8/Compose"

<Multi_key> <minus> <period> <period> 	: "‐"   U2010 # Hyphen
<Multi_key> <minus> <0> <0> 		: "‒"   U2012 # Figure dash
<Multi_key> <minus> <equal> <equal> 	: "―"   U2015 # Horizontal bar
